{
  "name": "bijous",
  "version": "0.3.0",
  "description": "An asynchronous module loader. Searches out modules within a file system using Klect and supplies an asynchronous means of initializing them. Initialized modules may provide a service that can be used by other modules.",
  "main": "lib/bijous.js",
  "engines": {
    "node": ">=0.10"
  },
  "dependencies": {
    "array-series": "^0.1.5",
    "klect": "^0.1.3",
    "underscore-plus": "^1.5.0"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "coffee-script": "^1.7.1",
    "gulp": "^3.8.5",
    "gulp-biscotto": "0.0.6",
    "gulp-coffee": "^2.0.1",
    "gulp-coffeelint": "^0.3.3",
    "gulp-istanbul": "^0.2.0",
    "gulp-mocha": "^0.4.1",
    "gulp-util": "^2.2.19",
    "sinon": "^1.10.3"
  },
  "scripts": {
    "test": "./node_modules/.bin/gulp test",
    "prepublish": "./node_modules/.bin/gulp build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mbrio/bijous.git"
  },
  "keywords": [
    "modules",
    "components",
    "features",
    "feats",
    "mods",
    "plugins",
    "async",
    "asynchronous",
    "require",
    "express"
  ],
  "author": "Michael Diolosa <michael.diolosa@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mbrio/bijous/issues"
  },
  "homepage": "https://github.com/mbrio/bijous"
}
